pipeline { 
  
   agent any

   stages {
   
     stage('Install Dependencies') { 
        steps { 
           sh 'echo "p1 install"'
        }
     }
     
     stage('Test') { 
        steps { 
           sh 'echo "p1 testing application..."'
        }
      }

         stage("Deploy application") { 
         steps { 
           sh 'echo "p1 deploying application..."'
         }

     }
  
   	}

   }